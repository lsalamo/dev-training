{
    "dimensions": [
        {
            "name": "eventName"
        },
        {
            "name": "customEvent:custom_link"
        }
    ],
    "metrics": [
        {
            "name": "eventCount"
        },
        {
            "name": "sessions"
        },
        {
            "name": "totalUsers"
        }
    ],
    "dateRanges": [
        {
            "startDate": "{{dt_from}}",
            "endDate": "{{dt_to}}"
        }
    ],
    "dimensionFilter": {
        "filter": {
            "fieldName": "platform",
            "stringFilter": {
                "matchType": "EXACT",
                "value": "{{platform}}"
            }
        }
    }
}