{
    "dimensions": [
        {
            "name": "date"
        },
        {
            "name": "platform"
        }
    ],
    "metrics": [
        {
            "name": "sessions"
        },
        {
            "name": "totalUsers"
        },
        {
            "name": "screenPageViews"
        }
    ],
    "dateRanges": [
        {
            "startDate": "{{dt_from}}",
            "endDate": "{{dt_to}}"
        }
    ],
    "dimensionFilter": {
        "filter": {
            "fieldName": "platform",
            "stringFilter": {
                "matchType": "EXACT",
                "value": "{{platform}}"
            }
        }
    },
    "orderBys": [
        {
            "dimension": {
                "orderType": "NUMERIC",
                "dimensionName": "date"
            }
        }
    ]
}