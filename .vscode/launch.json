    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Default",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH":"${workspaceFolder}/src/"
            }            
        },
        {
            "name": "Python: Google-Adobe",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            // args[4] - platform: web|android|ios
            "args":["mnet", "web", "7daysAgo", "today"],
            //"args":["mnet", "2024-12-01", "today", "ios", "11.113.0"],
            //"args":["mnet", "2024-12-01", "today", "android", "6.64.0"],
            "env": {
                "PYTHONPATH":"${workspaceFolder}/src/"
            }            
        }        
    ]
}